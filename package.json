{
  "name": "iniquity",
  "version": "0.0.20",
  "description": "A pure JavaScript command shell powered by Synchronet in Docker",
  "main": "iniquity.js",
  "scripts": {
    "start": "node_modules/.bin/concurrently --prefix \"[{name} {time}]\" -t \"HH:mm:ss\" -c \"bgGreen.bold,bgCyan.bold,bgMagenta.bold,bgBlue.bold,bgYellow.bold\" --kill-others-on-fail -n system,compiler \"docker-compose up\" \"tsc -p packages/app --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "lerna exec --concurrency 1 -- npx --no-install semantic-release -e semantic-release-monorepo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euphoriabbs/phoenix.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/euphoriabbs/phoenix/issues"
  },
  "homepage": "https://github.com/euphoriabbs/phoenix#readme",
  "dependencies": {
    "@types/node": "^14.6.0",
    "@types/terminal-kit": "^1.28.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "commander": "^6.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "2.2.1",
    "terminal-kit": "^1.42.0",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@semantic-release/npm": "^7.1.1",
    "lerna": "^4.0.0",
    "semantic-release": "^17.4.2",
    "semantic-release-monorepo": "^7.0.5"
  }
}
